{
    "name": "zfcampus/zf-apigility-skeleton",
    "description": "Skeleton Application for Apigility",
    "type": "library",
    "license": "BSD-3-Clause",
    "config": {
        "process-timeout": 900
    },
    "keywords": [
        "api",
        "apigility",
        "framework",
        "zf2"
    ],
    "homepage": "http://apigility.org/",
    "support": {
        "email": "apigility-users@zend.com",
        "irc": "irc://irc.freenode.net/apigility",
        "source": "https://github.com/zfcampus/zf-apigility-skeleton",
        "issues": "https://github.com/zfcampus/zf-apigility-skeleton/issues"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:etreedb/etreedb-db"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "dev-develop",
        "rwoverdijk/assetmanager": "~1.3",
        "zfcampus/zf-apigility": "1.0.x-dev",
        "zfcampus/zf-configuration": "1.0.x-dev",
        "zfcampus/zf-apigility-doctrine-admin": "dev-master",
        "zfcampus/zf-apigility-doctrine-server": "dev-master",
        "doctrine/doctrine-orm-module": "*",
        "hounddog/doctrine-data-fixture-module": "0.0.*",
        "fzaninotto/faker": "dev-master",
        "zendframework/zftool": "dev-master",
        "zendframework/zend-developer-tools": "dev-master",
        "zfcampus/zf-apigility-admin": "dev-master",
        "zfcampus/zf-apigility-welcome": "dev-master"
    }
}
